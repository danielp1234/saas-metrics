{
  "name": "saas-metrics-backend",
  "version": "1.0.0",
  "description": "Production-grade backend service for SaaS Metrics Benchmarking Platform with comprehensive security, monitoring, and scalability features",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc -p tsconfig.json",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "security:audit": "npm audit",
    "docker:build": "docker build -t saas-metrics-backend .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@bull-board/express": "^4.12.0",
    "@datadog/datadog-api-client": "1.0.x",
    "@types/node": "^22.12.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.0",
    "class-sanitizer": "0.14.x",
    "class-validator": "0.14.x",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.0",
    "express": "4.18.x",
    "express-rate-limit": "^6.9.0",
    "express-sanitizer": "1.x",
    "express-validator": "7.x",
    "google-auth-library": "^8.0.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.0.0",
    "joi": "17.x",
    "json-schema-to-ts": "^2.0.0",
    "jsonwebtoken": "9.x.x",
    "knex": "^2.4.0",
    "morgan": "^1.10.0",
    "numeral": "^2.0.6",
    "objection": "^3.0.0",
    "opossum": "^6.0.0",
    "pg": "^8.11.0",
    "prom-client": "^14.0.0",
    "rate-limit-redis": "^3.0.0",
    "response-time": "2.3.x",
    "sanitize-html": "^2.11.0",
    "swagger-ui-express": "5.0.x",
    "uuid": "^9.0.0",
    "validator": "^13.9.0",
    "winston": "^3.8.0",
    "winston-elasticsearch": "^0.17.1",
    "xss": "1.0.x",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@types/express": "^4.17.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "ioredis-mock": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.7",
    "redis-mock": "^0.56.3",
    "supertest": "^6.3.0",
    "testcontainers": "^9.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/organization/saas-metrics-platform.git"
  },
  "keywords": [
    "saas",
    "metrics",
    "benchmarking",
    "analytics",
    "typescript",
    "node",
    "express",
    "postgresql",
    "redis",
    "oauth"
  ],
  "author": "Organization Name",
  "license": "MIT"
}